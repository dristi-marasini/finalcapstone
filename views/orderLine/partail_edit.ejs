<div class=row>
  <div class="col col-xs-12">
    <div class="form-group">
      <label for="orderID">Order ID</label>
      <input type="text" class="form-control" id="orderID" name="orderID" value="<%= orderLineItem.orderID %>" required>
    </div>
    <div class="form-group">
      <label for="lineNumber">Line Number</label>
      <input type="text" class="form-control" id="lineNumber" name="lineNumber" value="<%= orderLineItem.lineNumber %>" required>
    </div>
    <div class="form-group">
      <label for="productKey">Product Key</label>
      <input type="text" class="form-control" id="productKey" name="productKey" value="<%= orderLineItem.productKey %>"
        required>
    </div>
    <div class="form-group">
      <label for="quantity">Quantity</label>
      <input type="text" class="form-control" id="quantity" name="quantity" value="<%= orderLineItem.quantity %>">
    </div>
  </div>
</div>